{
  "name": "react_repo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode prod",
    "lint": "echo \"(可选) 本项目未内置 ESLint 依赖，跳过 lint\"",
    "preview": "vite preview",
    "clean": "node -e \"const fs=require('fs');['dist','.vite','node_modules/.vite-temp'].forEach(p=>{try{fs.rmSync(p,{recursive:true,force:true});}catch(e){}});\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.4.5",
    "vite": "^5.4.0"
  }
}